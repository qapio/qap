
class NodeMeta {
  name: String?
}

class NodePosition {
  X: Number = 0
  Y: Number = 0
}

typealias Placement = "top" | "left" | "right" | "bottom"

open class Node {
  Type: String = "node"
  Id: String
  Content: String?
  Meta: NodeMeta = new NodeMeta {

  }
  Position: NodePosition = new NodePosition {

  }
  OutputPlacement: Placement?
  InputPlacement: Placement?
}

class Qap extends Node {

}

class Edge {
  From: String
  To: String
}

class Props {
  Nodes: List<Node>
  Edges: List<Edge>
}